[module property_call_query_source(http://www.eclipse.org/emf/2002/Ecore)/]
[template public test(c:EClass)]
	[file ('query_property_query', false)]
		[c.getContainer.name.toLowerLastQuery()/]
	[/file]
	[file ('query_property_template', false)]
		[c.getContainer.name.toLowerLastTemplate()/]
	[/file]
[/template]

[query public getContainer(e:ENamedElement) : ENamedElement = e.eContainer()/]
[query public toLowerLastQuery(s:String) : String = s.substring(1, s.size() - 1).concat(s.last(1).toLower())/]
[template public toLowerLastTemplate(s:String)][s.substring(1, s.size() - 1).concat(s.last(1).toLower())/][/template]
